<h1>New Employees  </h1>



<%= form_for @employee do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :classification%>
  <%= f.text_field :classification %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <%= f.submit %>
<% end %>

<%= button_to "foods", foods_path, method: :get %>
